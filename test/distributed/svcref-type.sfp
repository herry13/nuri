class Service {
	running is no
	procedure start {
		cost is 5
		conditions { }
		effects {
			this.running is yes
		}
	}
	procedure stop {
		cost is 10
		conditions { }
		effects {
			this.running is no
		}
	}
}

class WebService extends Service

class WebClient {
	refer isref WebService
	procedure change(s isref WebService) {
		conditions { }
		effects {
			this.refer is s
		}
	}
}
