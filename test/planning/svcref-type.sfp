class Machine {
	running is no
	procedure start {
		cost is 5
		condition { }
		effect {
			this.running is yes
		}
	}
	procedure stop {
		cost is 10
		condition { }
		effect {
			this.running is no
		}
	}
}

class Server extends Machine {
	name is ""
}

class Client {
	refer isref Server
	procedure change(s isref Server) {
		condition { }
		effect {
			this.refer is s
		}
	}
}
