include "svcref-type.sfp"

initial state {
	a isa Server {
		name is "A"
		running is true
	}
	b isa Server {
		name is "B"
	}
	c isa Server {
		name is "C"
	}
	pc1 isa Client {
		refer is a
	}
	pc2 isa Client {
		refer is a
	}
}
goal constraint {
	a.running is false
}
global constraint {
	pc1.refer.running is true
	pc1.refer in (a, c)
	pc2.refer.running is true
	pc2.refer not in (c)
}
