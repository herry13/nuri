class Client {
	refer isref WebService
	procedure change_ref(s isref WebService) {
		conditions { }
		effects {
			this.refer = s
		}
	}
}

class Runnable {
	running is false
	procedure start {
		conditions { }
		effects {
			this.running is true
		}
	}
	procedure stop {
		conditions { }
		effects {
			this.running is false
		}
	}
}

class Cloud

class Service extends Runnable {
	depend isset Service
	is_in isref VM
}

class VM extends Runnable {
	in_cloud isref Cloud
	procedure migrate(destination isref Cloud) {
		conditions {
			this.running is false
		}
		effects {
			this.in_cloud = destination
		}
	}
}

class WebService extends Service

class AppService extends Service

class DBService extends Service
