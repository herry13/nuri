include "../machine/machine.sfp"
include "../cloud/cloud.sfp"

schema vm extends machine {
	created = true
	in_cloud isref cloud

	final cpus = 1
	final memory = 512
	final networks { }
	final disks { }

	final flavor = ""
	final image = ""
	final security_group = ""

	final ssh_user = ""
	final ssh_key_name = ""
}
