include "../machine/machine.sfp"
include "../cloud/cloud.sfp"

schema VM extends Machine {
	created : Bool
	in_cloud isref Cloud
	final cpus = 1
	final memory = 512
	final network { }
	final disk { }
	final flavor = "default"
	final image = "default"
	final security_group = "default"
	final ssh_key = "default"
}
