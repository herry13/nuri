include "apache.sfp"

class ApacheLB extends Apache {
	children isset Apache

	procedure add_child (child isref Apache) {
		condition {
			child.running = true
		}
		effect {
			child in this.children
		}
	}

	procedure remove_child (child isref Apache) {
		condition { }
		effect {
			child not in this.children
		}
	}
}
