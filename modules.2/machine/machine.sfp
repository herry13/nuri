class Machine {
	running = false
	nuri_version = ""

	procedure upgrade_nuri (version isa String) {
		condition { }
		effect {
			this.nuri_version = version
		}
	}
	
	procedure start {
		condition { }
		effect {
			this.running = true
		}
	}

	procedure stop {
		condition { }
		effect {
			this.running = false
		}
	}
}
